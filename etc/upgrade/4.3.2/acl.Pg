sub acl {
    my $dbh = shift;

    my @acls;

    my @tables = qw (
      dashboards_id_seq
      Dashboards
    );

    foreach my $table (@tables) {
        push @acls,
          "GRANT SELECT, INSERT, UPDATE, DELETE ON $table to "
          . RT->Config->Get('DatabaseUser') . ";";

    }
    return (@acls);
}
1;
